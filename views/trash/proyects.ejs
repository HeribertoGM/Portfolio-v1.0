<!-- pre -->
<%- include('pre') -%>

<!-- custom CSS -->
<link rel="stylesheet" type="text/css" href="/static/css/proyects.css">

<title>HeribertoGM - Proyects</title>
</head>
<body>

<!-- Barra de navegacion -->
<%- include('navbar') -%>

<!-- Dentro -->
<div class="container" id="box">
	<h1 class="display-1 text-muted">Proyectos</h1>
	<% let rows = Math.floor(len/3); %>
	<% let rest = len%3; %>

	<% for(let i=rows; i>0; i--){ %>
		<% let index = [(i*3)-2,(i*3)-1,(i*3)]; %>
		<%- include('row', {data: data, indx: index}); -%>
	<% } %>

	<div class="row my-4">
		<% for(let i=rest; i>0; i--){ %>
			<% let index = rest-i; %>
			<%- include('col', {data: data, indx: index}); -%>
		<% } %>
	</div>
</div>

<!-- footer -->
<%- include('footer') -%>

<!-- post -->
<%- include('post') -%>