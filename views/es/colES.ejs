<div class="card col mx-2">
	<img src="<%- data[indx].img %>" class="card-img-top" alt="...">
	<div class="card-body">
		<h2 class="card-title text-muted"><%- data[indx].titleES %></h2>
		<p class="card-text">
			<% for(let i of data[indx].tags){ %>
				<span class="badge <%- i.type %>"><%- i.in %></span> 
			<% } %>
		</p>
		<a href="/proyects/<%- data[indx].id %>" class="btn btn-primary">Ir a Pagina del proyecto</a>
	</div>
</div>